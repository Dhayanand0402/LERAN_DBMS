SQL> create table student1(rollno number, name varchar2(20), totalmarks number, result varchar2(4));

SQL> insert into student1 values(&rollno, '&name', '&total', '&result'); //Repeat with 5 or more datas

SQL> select*from student1;

SQL>declare 
query1 varchar2(100):='create table Student1_pass as(select * from Student1 where result=''pass'')'; 
query2 varchar2(100):='create table Student1_fail as(select * from Student1 where result=''fail'')'; 
begin 
execute immediate query1; 
execute immediate query2; 
end; 
/ 

SQL>declare 
cursor cpass is select * from Student1_pass; 
cursor cfail is select * from Student1_fail; 
begin 
dbms_output.put_line('LIST OF PASSED STUDENTS'); 
for passrec in cpass loop 
dbms_output.put_line(passrec.name); 
end loop; 
dbms_output.put_line('LIST OF FAILED STUDENTS'); 
for failrec in cfail loop 
dbms_output.put_line(failrec.name); 
end loop; 
end; 
/

